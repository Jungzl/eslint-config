{
  "name": "@jungzl/eslint-config-ts",
  "version": "0.0.2",
  "description": "jungzl's ESLint configuration for TypeScript",
  "author": "jungzl",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jungzl",
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "release": "bumpp package.json --commit \"release @jungzl/eslint-config-ts v%s\" --push --tag \"ts/v%s\" && pnpm publish --access public"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@jungzl/eslint-config-basic": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1"
  }
}
