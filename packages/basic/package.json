{
  "name": "@jungzl/eslint-config-basic",
  "version": "0.0.2",
  "description": "jungzl's basic eslint config",
  "author": "jungzl",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jungzl",
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "release": "bumpp package.json --commit \"release @jungzl/eslint-config-basic v%s\" --push --tag \"basic/v%s\" && pnpm publish --access public"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-ecmascript-compat": "^2.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-jsonc": "^2.3.1",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "eslint-plugin-yml": "^1.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "yaml-eslint-parser": "^1.0.1"
  }
}
