{
  "name": "@jungzl/eslint-config-vue",
  "version": "0.0.2",
  "description": "jungzl's ESLint config for Vue",
  "author": "jungzl",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jungzl",
    "eslint",
    "eslint-config"
  ],
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint . --config=index.js",
    "release": "bumpp package.json --commit \"release @jungzl/eslint-config-vue v%s\" --push --tag \"vue/v%s\" && pnpm publish --access public"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@jungzl/eslint-config-ts": "workspace:*",
    "eslint-plugin-vue": "^9.2.0"
  }
}
