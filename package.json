{
  "name": "@jungzl/eslint-config-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "jungzl's ESLint configuration monorepo",
  "author": "jungzl",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "update": "pnpm up -r -i --latest",
    "lint": "eslint . --fix",
    "release:basic": "pnpm -F *basic release",
    "release:ts": "pnpm -F *ts release",
    "release:react": "pnpm -F *react release",
    "release:vue": "pnpm -F *vue release",
    "release:solid": "pnpm -F *solid release"
  },
  "devDependencies": {
    "@jungzl/eslint-config-ts": "workspace:*",
    "bumpp": "^9.0.0",
    "eslint": "^8.24.0",
    "typescript": "^4.8.4"
  }
}
